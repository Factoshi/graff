{
    "name": "factom-graph",
    "version": "0.1.0",
    "description": "GraphQL translation layer for the factomd RPC API.",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src",
        "start-watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.js' --exec 'ts-node' src",
        "build": "tsc",
        "test": "jest",
        "test-query-mutation": "jest --config='test/queryMutation/jest.config.js'",
        "test-subscription": "factom-dev-stack wrap -c test/subscription/.factomds.json \"FACTOMD_PORT=5088 jest --config='test/subscription/jest.config.js'\"",
        "generate": "graphql-codegen --config codegen.yml",
        "generate-init": "graphql-codegen init"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Factoshi/factom-graph.git"
    },
    "keywords": [
        "factom",
        "api",
        "graphql"
    ],
    "author": "Alex Carrithers (Factoshi Ltd)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Factoshi/factom-graph/issues"
    },
    "homepage": "https://github.com/Factoshi/factom-graph#readme",
    "devDependencies": {
        "@graphql-codegen/cli": "^1.3.1",
        "@graphql-codegen/typescript": "1.2.0",
        "@graphql-codegen/typescript-resolvers": "1.2.0",
        "@types/chai": "^4.1.7",
        "@types/jest": "^24.0.15",
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link-ws": "^1.0.18",
        "apollo-server-testing": "^2.6.8",
        "chai": "^4.2.0",
        "factom-dev-stack": "^0.2.2",
        "graphql-tag": "^2.10.1",
        "jest": "^24.8.0",
        "nodemon": "^1.19.1",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.2"
    },
    "dependencies": {
        "@types/basic-auth": "^1.1.2",
        "@types/bluebird": "^3.5.27",
        "@types/graphql": "^14.2.2",
        "@types/ramda": "^0.26.15",
        "apollo-server": "^2.6.7",
        "basic-auth": "^2.0.1",
        "bluebird": "^3.5.5",
        "dataloader": "^1.4.0",
        "factom": "^1.2.4",
        "graphql": "^14.4.0",
        "graphql-import": "^0.7.1",
        "ramda": "^0.26.1"
    }
}
